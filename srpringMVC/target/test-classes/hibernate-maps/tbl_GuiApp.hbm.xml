        </property>
        <property name="appstep" type="string">
            <column name="APPSTEP" length="10" not-null="true" />
        </property>
        <property name="appflag" type="string">
            <column name="APPFLAG" length="10" not-null="true" />
        </property>
        <property name="appmessage" type="string">
            <column name="APPMESSAGE" length="1000" />
        </property>
        <property name="appattachments" type="string">
            <column name="APPATTACHMENTS" length="200" />
        </property>
        <property name="appattachmentcount" type="long">
            <column name="APPATTACHMENTCOUNT" precision="22" scale="0" not-null="true" />
        </property>
        <property name="appiter" type="long">
            <column name="APPITER" precision="22" scale="0" not-null="true" />
        </property>
        <property name="applock" type="long">
            <column name="APPLOCK" precision="22" scale="0" not-null="true" />
